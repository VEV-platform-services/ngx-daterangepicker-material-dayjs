{
    "name": "ngx-daterangepicker-material-dayjs",
    "version": "4.1.0",
    "description": "Angular 16 date range picker (with material design theme)",
    "keywords": [
        "angular2",
        "angular16",
        "ng2",
        "daterangepicker",
        "material",
        "component",
        "directive",
        "daterange",
        "date",
        "moment",
        "picker"
    ],
    "homepage": "https://github.com/gjsmith66/ngx-daterangepicker-material-dayjs",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --port=4201",
        "demo:build": "ng build --configuration production",
        "test": "./node_modules/.bin/ng test --watch=false",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "ngc": "./node_modules/.bin/ngc -p tsconfig.json",
        "gh-pages": "ng build --configuration production --base-href=\"/ngx-daterangepicker-material-dayjs/\" && ngh --dir demo/dist",
        "build": "ng-packagr -p ng-package.json"
    },
    "author": "Smith G.",
    "repository": {
        "type": "git",
        "url": "https://github.com/gjsmith66/ngx-daterangepicker-material-dayjs.git"
    },
    "peerDependencies": {
        "dayjs": "^1.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^16.0.3",
        "@angular/animations": "^16.0.3",
        "@angular/cdk": "16",
        "@angular/cli": "16.0.3",
        "@angular/common": "^16.0.3",
        "@angular/compiler": "^16.0.3",
        "@angular/compiler-cli": "^16.0.3",
        "@angular/core": "^16.0.3",
        "@angular/forms": "^16.0.3",
        "@angular/material": "^15.2.9",
        "@angular/platform-browser": "^16.0.3",
        "@angular/platform-browser-dynamic": "^16.0.3",
        "@angular/router": "^16.0.3",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "angular-cli-ghpages": "^1.0.0-rc.1",
        "codelyzer": "^6.0.0",
        "core-js": "^2.5.4",
        "dayjs": "^1.10.4",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.4.2",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "materialize-css": "^1.0.0",
        "ng-packagr": "^16.0.1",
        "ngh": "^0.2.12",
        "protractor": "~7.0.0",
        "rimraf": "^2.6.1",
        "rxjs": "^6.6.3",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.0",
        "typescript": "~4.9.5",
        "zone.js": "~0.13.0"
    },
    "$schema": "./node_modules/ng-packagr/package.schema.json",
    "ngPackage": {
        "lib": {
            "entryFile": "src/daterangepicker/index.ts"
        },
        "dest": "dist"
    },
    "allowNonPeerDependencies": {
        "@angular-devkit/core": "16"
    }
}
